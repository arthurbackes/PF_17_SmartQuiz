<h2>Search List</h2>
<form action="/list" method="GET">
    <label for="name">Name</label>
    <input type="text" name="name">
    <button type="submit">Search</button>
</form>
<br>
<% lists.forEach(list => { %>
    <div>
        <!-- Crée un lien vers l'ID spécifique -->
        <a href="/list/<%= list._id %>"><%= list.name %></a>
    </div>
<% }) %>
