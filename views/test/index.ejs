<section class="flex flex-col justify-center items-center">
    <h1 class="font-bold text-4xl mt-6 mb-2 text-deepBlue">Choisir une liste pour faire un test</h1>
    <div class="mt-6 flex flex-col w-full max-w-lg">
        <% lists.forEach(list => { %>

            <a class="border bg-white border-oceanBlue rounded-lg p-4 mb-4 text-center text-lg font-medium text-deepBlue hover:bg-oceanBlue hover:text-white hover:border-oceanBlue transition-all" href="/test/<%= list._id %>/"><%= list.name %></a>

        <% }) %>
    </div>
</section>